source 201.th

input ../../layouts/metapost/scalebar_arrow.thl
layout local
  copy custom_scalebar
  scale 1 5000
  grid top
  grid-size 5 5 5 m
  symbol-colour point station-name [20 20 80]
  colour map-fg [90 90 90]
  map-header 0 0 nw
endlayout

export atlas -o 201_plan.pdf -layout local -layout-debug station-names
